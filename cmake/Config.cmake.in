@PACKAGE_INIT@
## COMMON SETTINGS ##
set(PROJECT_MODULE_NAME intern_repo)

## DEPENDENCIES CONFIGURATIONS ##
set(LIB_A_PACKAGE_NAME LibA)
set(LIB_A_LIB_NAME LibA)
set(LIB_A_LIB_VERSION 1.0.0)

set(LIB_A_IMPORT_LOC "${INSTALL_ROOT_DIR}/${PROJECT_MODULE_NAME}/${LIB_A_PACKAGE_NAME}/v${LIB_A_LIB_VERSION}/${CMAKE_SYSTEM_NAME}/${CMAKE_SYSTEM_VERSION}/cmake")

list(APPEND CMAKE_PREFIX_PATH ${LIB_A_IMPORT_LOC})

include(CMakeFindDependencyMacro)
find_dependency(${LIB_A_PACKAGE_NAME} ${LIB_A_LIB_VERSION} REQUIRED CONFIG)

## BUILD CONFIGURATIONS ##
set(LIB_B_TARGET_NAME LibB)

include(${CMAKE_CURRENT_LIST_DIR}/${LIB_B_TARGET_NAME}Targets.cmake)

set(${LIB_B_TARGET_NAME}_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIR@")
set(${LIB_B_TARGET_NAME}_LIBRARIES ${LIB_B_TARGET_NAME}::${LIB_B_TARGET_NAME})

check_required_components(${LIB_B_TARGET_NAME})